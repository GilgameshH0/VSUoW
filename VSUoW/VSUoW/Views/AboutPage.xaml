<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="VSUoW.Views.AboutPage"
             xmlns:vm="clr-namespace:VSUoW.ViewModels"
             Title="О приложении">
    
    <ContentPage.BindingContext>
        
    </ContentPage.BindingContext>

    <ContentPage.Resources>
        <ResourceDictionary>
            <!--Page Level Resources: Compatibile with Xamarin Live Player -->
            <Color x:Key="Primary">#2196F3</Color>
            <Color x:Key="Accent">#96d1ff</Color>
            <Color x:Key="LightTextColor">#999999</Color>
        </ResourceDictionary>
    </ContentPage.Resources>
    <ContentPage.Content>
        
        <Grid Padding="10">
            <Grid.RowDefinitions>
               
                <RowDefinition Height="Auto" />
               
            </Grid.RowDefinitions>
            <StackLayout Grid.Column="0" Grid.Row="0" >
                <Image Source="Olegq.jpg"/>
                <Label Text="Руководитель проекта: Шилкин О.С."/>
            </StackLayout>
            <StackLayout Grid.Column="1" Grid.Row="0">
                <Image Source="Alexeyq.jpg" />
            <Label Text="Заместитель руководителя проекта: Шомов А.И."/>
            </StackLayout>
        </Grid>
    </ContentPage.Content>
</ContentPage>